{
  "name": "barkleysstore",
  "version": "1.0.0",
  "description": "Where you feel like shopping",
  "main": "index.js",
  "scripts": {
    "start": "node app/server.js",
    "server:watch": "nodemon app/server.js",
    "server:open&watch": "start http://localhost:3000 & nodemon app/server.js",
    "server:open": "start http://localhost:3000",
    "client": "npm run dev --prefix client -c=cyan",
    "client:run": "live-server client/dist",
    "client:prod": "npm run build --prefix client",
    "dev": "concurrently -n \"react,node,Browser\" -c \"bgCyan,bgGreen,bgMagenta\" \"npm run client\" \"npm run server:watch\" \"npm run server:open\""
  },
  "engines": {
    "node": "14.15.1"
  },
  "author": "Joraav",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.5",
    "mongoose": "^5.7.7",
    "multer": "^1.4.2",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
